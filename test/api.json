// GET https/api/pageContent
// API для данных на страниц Любители, Дети, Студенты
{
    "body": {
        "page": "Любители"  
        // структура данных у страниц похожи, но разные сами данные
    },
    "response": {
        "images": [
            {
                "id":"1",
                "url":"https/api/Название_картинки"
            }
            // ...
            // картинок будет много и желательно чтобы хранились на сервере
        ],
        "video": [
            {
                "id":"1",
                "url":"https://www.youube.com/"
            }
            // ...
            // Видео предполагаю будут храниться на ютубе
        ],
        "treners": [
            {
                "id":"1",
                "img": " https/api/Картинка_с_тренером",
                "name": "Иван Иванович",
                "rank": "Мастер Спорта",
                "phone_number":"+79990001122"
            }
            // ...
            // Тренеров будет несколько
        ],
        "time": [
            {
                "id": "1",
                "data":["-", "12.00-13.00", "-", "-", "-", "-", "-"]
            }
            // ...
            // Групп будет несколько даты указываеться по дням недели
        ],
        "price": [
            {
                "id":"1",
                "title": "Разовое занятие",
                "price": "800"
            }
            // вариантов цен и занятий может быть несколько
        ],
        "feedback": [
            {
                "id":"1",
                "name": "Иванов Иванов",
                "ball": "5",
                "description": "Мне всё понравилось"
            }
            // отзывов будет несколько
        ]
    }
}

