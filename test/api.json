// GET https/api/pageNews
// API для страниц с Новостями
{
    "body": {
        "start": "1"  ,
        "end": "10"
        // определённое количество новостей нужно для пагинации
    },
    "response": {
        "news": [
            {
                "id":"1",
                "title":"Текст", // текст не весь, а всеволишь 5-10 строк
                "time":"27.08.223",
                "image": {
                        "id":"1",
                        "url":"https/api/Название_картинки"
                    }
                    // приходит только одна картинка
            }
            // ...
            // Новостей будет несколько
        ]
    }
}

